@model IEnumerable<FSite.Models.Data.SliderItem>
@if (Model.Count() != 0)
{
    <div class="collectionBanner insScroll animated zoomIn">
        <div class="banner">
            @foreach (var item in Model)
                {
                <img src="/Uploads/images/BlogCategories/@item.Title" class="insImageload" />
            }
        </div>
    </div>
   }