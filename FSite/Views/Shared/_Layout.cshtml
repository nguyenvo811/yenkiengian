    <script>
        if ($(".pricerange-box-js").length !== 0) {
            var pricenums = [0, 1, 5, 10, 15, 20, 25, 30, 50, 100, 200, 300, 400, 500, 600, 700, 800, 900, 1000, 2000, 3000, 4000, 5000, 6000, 7000, 8000, 9000, 10000, 20000, 30000, 40000, 50000, 60000, 70000, 80000, 90000, 100000];
            var c = pricenums.length;//count item price num to for memmory
            $pricerange = $(".pricerange-box-js");
            $pricerange.ionRangeSlider({
                // grid: true,
                type: "double",
                // from: 5,
                // keyboard: true,
                values: pricenums,
                // postfix: " vnd",
                hide_min_max: true,
                hide_from_to: false,
                //    max_postfix: "+",
                decorate_both: false,
                values_separator: " → ",
                prettify_enabled: true,
                prettify: formatPriceRange,
                onFinish: function (data) {
                    //setTimeout(submitProductSearchBox,500);
                },
            });
            function formatPriceRange(num) {
                if (num === 0)
                    return num + " vnd";
                if (num < 1000)//chưa tới tỷ
                    return num + " triệu vnd";
                if (num === pricenums[c - 1])//max
                    return (num / 1000) + " tỷ vnd +";
                return (num / 1000) + " tỷ vnd";//tới tiền tỷ thì bỏ 3 số 0 => /1000 for pretty on display
            }

            $pricerange.on("change", function () {
                var $this = $(this),
                    from = $this.data("from"),//index [from] array pricenums
                    to = $this.data("to");//index [to] array pricenums
                //empty input [from] when return 0
                if (from === 0)
                    $this.find('.range-from-js').val('');
                else $this.find('.range-from-js').val(pricenums[from]);
                //empty input [from] when return max length
                if (to === c - 1)
                    $this.find('.range-to-js').val('');
                else $this.find('.range-to-js').val(pricenums[to]);
                // when post from and to need to * 1 000 000 => 6 zero and compare with value
            });


            function priceUpdate() {
                var priceFrom = document.getElementById('PriceFrom').value;
                var priceTo = document.getElementById('PriceTo').value;
                if (!priceFrom) {
                    priceFrom = pricenums[0];
                }
                if (!priceTo) {

                    priceTo = pricenums[pricenums.length - 1];
                }
                getIndexPrice = function (v) {
                    return pricenums.findIndex(function (obj) {
                        return obj + '' === v + '';
                    })
                }
                var _ipriceFrom = getIndexPrice(priceFrom);
                var _ipriceTo = getIndexPrice(priceTo);
                $pricerange.data("ionRangeSlider").update({
                    from: _ipriceFrom,
                    to: _ipriceTo,
                });
            }
            priceUpdate();
        }
    </script>
